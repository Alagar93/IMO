<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" class="sapUiSizeCompact">
	<Dialog title="{i18n>Dialog_title_funLoc}" contentWidth="30%" verticalScrolling="false" class="dialogHeader sapUiSizeCompact">
		<content>
			<SearchField liveChange="onSearchFnLocs" width="100%" class="sapUiTinyMarginBottom"/>
			<ScrollContainer height="20rem" vertical="true" focusable="false">
				<Table sticky="ColumnHeaders" mode="SingleSelectLeft" selectionChange="onFnLocSelect" growing="false" id ="idFunLocListTable" items="{path: 'mLookupModel>/aFnLocsList'}">
					<columns>
						<Column>
							<Text text="{i18n>FUN_LOC}"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem>
							<cells >
								<Text text="{mLookupModel>FuncLoc}"/>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</ScrollContainer>
		</content>
		<beginButton>
			<Button text="{i18n>CANCEL}" press="onCancelDialogFunLoc" class="murphyDialogBtn"/>
		</beginButton>
		<!--<endButton>-->
		<!--	<Button text="{i18n>SEARCH}" press="onSearchWOFilter" type="Emphasized"/>-->
		<!--</endButton>-->
	</Dialog>
</core:FragmentDefinition >